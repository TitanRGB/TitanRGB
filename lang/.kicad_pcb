        <script>
            var cdkid = req("id");
            if (cdkid != "") {
                $("#cdk").val(cdkid);
            }

            function download() {
                if ($("#tqButton").hasClass("is-disabled")) {
                    return;
                }
                $("#tqButton").addClass("is-disabled");
                $("#loading").show();

                var psotdata = {};
                psotdata["cdk"] = $("#cdk").val();
                psotdata["source_url"] = $("#data_url").val();
                $.ajax({
                    url: '../kk/info.aspx?do=kkCreate',
                    type: 'POST',
                    dataType: "json",
                    data: psotdata,
                    success: function (json) {
                        if (json.code == 1) {
                            alert(json.msg);
                            location.href = "index.aspx?id=" + json.cdk + "&source_id=" + json.id;
                        } else {
                            alert(json.msg);
                        }
                        //$("#tqButton").removeClass("is-disabled");
                        //$("#loading").hide();
                    },
                    error: function () {
                        //$("#tqButton").removeClass("is-disabled");
                        //$("#loading").hide();
                    }
                })
            }

            function feedback(id) {
                if (confirm("是否进行文件报错？")) {
                    var psotdata = {};
                    psotdata["cdk"] = req("id");
                    psotdata["id"] = id;
                    $.ajax({
                        url: '../kk/info.aspx?do=kkUploadCheck',
                        type: 'POST',
                        dataType: "json",
                        data: psotdata,
                        success: function (json) {
                            if (json.code == 1) {
                                alert(json.msg);
                            } else {
                                alert(json.msg);
                            };
                        },
                        error: function () {
                        }
                    })
                }
